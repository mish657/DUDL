     ]
    }
   ],
   "source": [
    "x=[1,2,4,6,5,4,0]\n",
    "h=len(x)\n",
    "\n",
    "mean1=np.mean(x)\n",
    "mean2=np.sum(x)/h\n",
    "\n",
    "print(mean1)\n",
    "print(mean2)"
   ]
  },
  {
