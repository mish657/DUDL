     ]
    }
   ],
   "source": [
    "#variance\n",
    "\n",
    "var1=np.var(x,ddof=1)\n",
    "var2=(1/(h-1))*np.sum((x-mean1)**2)\n",
    "\n",
    "print(var1)\n",
    "print(var2)"
   ]
